<?xml version="1.0"?>
<launch>
  <!-- roslaunch arguments -->
  <arg name="paused" default="false"/>
  <arg name="debug" default="false"/>
  <arg name="gui" default="true"/>

  <include file="$(find simplified)/launch/world_sim.launch">
    <arg name="paused" value="$(arg paused)" />
    <arg name="debug" value="$(arg debug)" />
    <arg name="gui" value="$(arg gui)" />
  </include>

  <include file="$(find simplified)/launch/joint_controllers.launch"></include>

  <rosparam command="load" file="$(find simplified)/configs/vehicle_params.yaml" />

  <node name="base_controller1" pkg="simplified" type="base_controller" output="screen"> </node>

<!--   <node name="odom_sim" pkg="simplified" type="odom_simulator" output="screen"> </node> -->
  
<!--   <include file="$(find walkernew)/launch/kinect.launch">
  </include> -->

<!--   <include file="$(find walkernew)/launch/walkernew_rviz.launch">
  </include> -->

</launch>
